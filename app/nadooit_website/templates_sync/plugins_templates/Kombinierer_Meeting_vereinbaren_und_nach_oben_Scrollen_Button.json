    "name": "Kombinierer_Meeting_vereinbaren_und_nach_oben_Scrollen_Button",
    "html": "<body>\r\n\r\n    <div class=\"position-fixed bottom-0 end-0 m-3\">\r\n\r\n        <div class=\"btn-group-vertical\" role=\"group\">\r\n\r\n            <button id=\"meetingBtn\" title=\"Meeting vereinbaren\" class=\"btn btn-primary\" data-bs-toggle=\"modal\" data-bs-target=\"#meetingModal\">Meeting</button>\r\n\r\n            <button onclick=\"topFunction()\" id=\"myBtn\" title=\"Go to top\" class=\"btn btn-primary\"><i class=\"fal fa-arrow-up\"></i> Nach oben scrollen</button>\r\n\r\n        </div>\r\n\r\n    </div>\r\n\r\n\r\n\r\n    <!-- Modal -->\r\n\r\n    <div class=\"modal fade\" id=\"meetingModal\" tabindex=\"-1\" aria-labelledby=\"meetingModalLabel\" aria-hidden=\"true\">\r\n\r\n        <div class=\"modal-dialog\">\r\n\r\n            <div class=\"modal-content\">\r\n\r\n                <div class=\"modal-header\">\r\n\r\n                    <h5 class=\"modal-title\" id=\"exampleModalLabel\">Meeting vereinbaren</h5>\r\n\r\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n\r\n                </div>\r\n\r\n                <div class=\"modal-body\">\r\n\r\n                    Hier k\u00f6nnen Sie ein Meeting vereinbaren.\r\n\r\n                </div>\r\n\r\n                <div class=\"modal-footer\">\r\n\r\n                    <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Schlie\u00dfen</button>\r\n\r\n                    <button type=\"button\" class=\"btn btn-primary\">Meeting anfragen</button>\r\n\r\n                </div>\r\n\r\n            </div>\r\n\r\n        </div>\r\n\r\n    </div>\r\n\r\n\r\n\r\n    <script>\r\n\r\n        function topFunction() {\r\n\r\n            document.body.scrollTop = 0;\r\n\r\n            document.documentElement.scrollTop = 0;\r\n\r\n        }\r\n\r\n    </script>"
}
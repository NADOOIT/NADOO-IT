    "name": "Kombinierer_Meeting_vereinbaren_und_nach_oben_Scrollen_Button",
    "html": "<body>\n\n\n\n    <div class=\"position-fixed bottom-0 end-0 m-3\">\n\n\n\n        <div class=\"btn-group-vertical\" role=\"group\">\n\n\n\n            <button id=\"meetingBtn\" title=\"Meeting vereinbaren\" class=\"btn btn-primary\" data-bs-toggle=\"modal\" data-bs-target=\"#meetingModal\">Meeting</button>\n\n\n\n            <button onclick=\"topFunction()\" id=\"myBtn\" title=\"Go to top\" class=\"btn btn-primary\"><i class=\"fal fa-arrow-up\"></i> Nach oben scrollen</button>\n\n\n\n        </div>\n\n\n\n    </div>\n\n\n\n\n\n\n\n    <!-- Modal -->\n\n\n\n    <div class=\"modal fade\" id=\"meetingModal\" tabindex=\"-1\" aria-labelledby=\"meetingModalLabel\" aria-hidden=\"true\">\n\n\n\n        <div class=\"modal-dialog\">\n\n\n\n            <div class=\"modal-content\">\n\n\n\n                <div class=\"modal-header\">\n\n\n\n                    <h5 class=\"modal-title\" id=\"exampleModalLabel\">Meeting vereinbaren</h5>\n\n\n\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n\n\n\n                </div>\n\n\n\n                <div class=\"modal-body\">\n\n\n\n                    Hier k\u00f6nnen Sie ein Meeting vereinbaren.\n\n\n\n                </div>\n\n\n\n                <div class=\"modal-footer\">\n\n\n\n                    <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Schlie\u00dfen</button>\n\n\n\n                    <button type=\"button\" class=\"btn btn-primary\">Meeting anfragen</button>\n\n\n\n                </div>\n\n\n\n            </div>\n\n\n\n        </div>\n\n\n\n    </div>\n\n\n\n\n\n\n\n    <script>\n\n\n\n        function topFunction() {\n\n\n\n            document.body.scrollTop = 0;\n\n\n\n            document.documentElement.scrollTop = 0;\n\n\n\n        }\n\n\n\n    </script>"
}